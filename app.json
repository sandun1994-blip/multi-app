{
  "expo": {
    "name": "devember-one",
    "experiments": {
      "tsconfigPaths": true
    },
    "slug": "devember-one",
    "scheme": "your-app-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sandun1rn.devemberone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sandun1rn.devemberone"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "react-native-health",
      "expo-local-authentication"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "56826a5b-72ce-4198-b5c0-5110a9a00bdd"
      }
    }
  }
}
